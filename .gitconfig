[core]
  editor = vim

[color]
  ui = auto

# ~/.gitconfig - template for students. Fill in name and email after forking.
[user]
  name = Your Name Here
  email = you@example.com

# Tips:
#  - Update your name and email:
#      git config --global user.name "Your Name"
#      git config --global user.email you@example.com
#  - This file is safe to commit into a classroom repo as a template; each student should
#    replace the placeholder values with their own.